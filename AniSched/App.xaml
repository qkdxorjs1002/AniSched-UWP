<Application
    x:Class="AniSched.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:AniSched"
    RequestedTheme="Light">
    <Application.Resources>
        <SolidColorBrush x:Key="customBackgroundColor" Color="#FFFAFAFA"/>
        <SolidColorBrush x:Key="customBaseColor" Color="#FFFE3B72"/>
        <SolidColorBrush x:Key="customAccentForgroundColor" Color="#FFFAFAFA"/>
        <SolidColorBrush x:Key="customAccentForgroundColor_alt" Color="#FF000000"/>
        <SolidColorBrush x:Key="customAccentBackgroundColor" Color="#FFF02F65"/>
        <SolidColorBrush x:Key="customAccentBackgroundColor_alt" Color="#FFE6E6E6"/>
    </Application.Resources>
</Application>
